{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug trainAI",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/AI/trainAI.out", // Replace with the path to your executable
        "args": [], // Any arguments you want to pass to your program
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true, // Change to true if you want an external console window
        "MIMode": "gdb", // or "lldb" on macOS, depending on the debugger you are using
        "miDebuggerPath": "/usr/bin/gdb", // Path to gdb (or lldb)
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }
  